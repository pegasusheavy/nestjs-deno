#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/vitest@4.0.16_jiti@2.6.1_jsdom@27.4.0_lightningcss@1.30.2_sass@1.93.2/node_modules/vitest/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/vitest@4.0.16_jiti@2.6.1_jsdom@27.4.0_lightningcss@1.30.2_sass@1.93.2/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/vitest@4.0.16_jiti@2.6.1_jsdom@27.4.0_lightningcss@1.30.2_sass@1.93.2/node_modules/vitest/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/vitest@4.0.16_jiti@2.6.1_jsdom@27.4.0_lightningcss@1.30.2_sass@1.93.2/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vitest@4.0.16_jiti@2.6.1_jsdom@27.4.0_lightningcss@1.30.2_sass@1.93.2/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/vitest@4.0.16_jiti@2.6.1_jsdom@27.4.0_lightningcss@1.30.2_sass@1.93.2/node_modules/vitest/vitest.mjs" "$@"
fi
