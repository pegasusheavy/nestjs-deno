#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/@angular+cli@21.0.4_chokidar@4.0.3/node_modules/@angular/cli/bin/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/@angular+cli@21.0.4_chokidar@4.0.3/node_modules/@angular/cli/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/@angular+cli@21.0.4_chokidar@4.0.3/node_modules/@angular/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/@angular+cli@21.0.4_chokidar@4.0.3/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/@angular+cli@21.0.4_chokidar@4.0.3/node_modules/@angular/cli/bin/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/@angular+cli@21.0.4_chokidar@4.0.3/node_modules/@angular/cli/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/@angular+cli@21.0.4_chokidar@4.0.3/node_modules/@angular/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/@angular+cli@21.0.4_chokidar@4.0.3/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/ng.js" "$@"
else
  exec node  "$basedir/../../bin/ng.js" "$@"
fi
