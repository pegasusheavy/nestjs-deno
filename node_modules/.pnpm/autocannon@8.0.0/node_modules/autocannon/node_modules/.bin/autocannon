#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/joseph/PegasusHeavyIndustries/nestjs-deno/node_modules/.pnpm/autocannon@8.0.0/node_modules/autocannon/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/node_modules/.pnpm/autocannon@8.0.0/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/joseph/PegasusHeavyIndustries/nestjs-deno/node_modules/.pnpm/autocannon@8.0.0/node_modules/autocannon/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/node_modules/.pnpm/autocannon@8.0.0/node_modules:/home/joseph/PegasusHeavyIndustries/nestjs-deno/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../autocannon.js" "$@"
else
  exec node  "$basedir/../../autocannon.js" "$@"
fi
